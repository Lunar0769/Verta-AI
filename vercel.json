{
  "version": 2,
  "builds": [
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/analyze.py",
      "use": "@vercel/python"
    },
    {
      "src": "website/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/analyze",
      "dest": "/api/analyze.py"
    },
    {
      "src": "/(.*)",
      "dest": "/website/$1"
    }
  ]
}